{
  "name": "@hazelcast/ui",
  "version": "0.0.1",
  "description": "> TODO: description",
  "author": "Andrey Goncharov <andrey@goncharov.page>",
  "homepage": "https://github.com/hazelcast/frontend-shared#readme",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "styles"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hazelcast/frontend-shared.git"
  },
  "scripts": {
    "tsc": "tsc -p ./tsconfig.publish.json",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish-storybook": "chromatic --storybook-build-dir storybook-static --project-token $CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "test:visual": "loki --requireReference --reactUri file:./storybook-static --chromeFlags='--headless --disable-gpu --hide-scrollbars --no-sandbox --allow-file-access-from-files'",
    "generate-screenshots": "loki update --reactUri file:./storybook-static --chromeFlags='--headless --disable-gpu --hide-scrollbars --no-sandbox --allow-file-access-from-files'"
  },
  "bugs": {
    "url": "https://github.com/hazelcast/frontend-shared/issues"
  },
  "dependencies": {
    "@hazelcast/helpers": "^0.0.1",
    "@hazelcast/services": "^0.0.1",
    "classnames": "^2.2.6",
    "formik": "^2.2.0",
    "rc-tooltip": "^4.2.3",
    "react-feather": "^2.0.8",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@hazelcast/test-helpers": "^0.0.1",
    "@types/classnames": "^2.2.10",
    "@types/uuid": "^8.3.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
